<!DOCTYPE html>
<html>
<head>
    <title>Discord Proxy</title>
</head>
<body>
    <iframe src="https://discord.com" 
            width="100%" height="100vh" 
            frameborder="0" 
            style="position:fixed;top:0;left:0;right:0;bottom:0;z-index:9999">
    </iframe>
    <script>
    // Fallback proxy fetch
    document.body.innerHTML += '<div id="proxy">Loading Discord...</div>';
    fetch('https://discord.com', {mode:'cors'})
    .then(r=>r.text()).then(html=>document.getElementById('proxy').innerHTML=html);
    </script>
</body>
</html>
